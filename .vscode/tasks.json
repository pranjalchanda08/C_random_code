{
    "tasks": [
        {
            "type": "shell",
            "command": "mkdir -p ${workspaceFolder}/Examples/build",
            "label": "Make dir build",
        },
        {
            "type": "cppbuild",
            "label": "Build cq_test",
            "dependsOn": "Make dir build",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "test_fifo_circular.c",
                "${workspaceFolder}/Examples/../library/FIFO/*",
                "-I${workspaceFolder}/Examples/../library/FIFO/",
                "-o",
                "${workspaceFolder}/Examples/build/cq_test"
            ],
            "options": {
                "cwd": "${workspaceFolder}/Examples"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "Build fifo_test",
            "dependsOn": "Make dir build",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "test_fifo_linear.c",
                "${workspaceFolder}/Examples/../library/FIFO/*",
                "-I${workspaceFolder}/Examples/../library/FIFO/",
                "-o",
                "${workspaceFolder}/Examples/build/fifo_test"
            ],
            "options": {
                "cwd": "${workspaceFolder}/Examples"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "Build sort_test",
            "dependsOn": "Make dir build",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/Examples/test_sort.c",
                "${workspaceFolder}/Examples/../library/sort/*",
                "-I${workspaceFolder}/Examples/../library/sort/",
                "-o",
                "${workspaceFolder}/Examples/build/sort_test"
            ],
            "options": {
                "cwd": "${workspaceFolder}/Examples"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "Build all",
            "dependsOn": [
                "Make dir build",
                "Build cq_test",
                "Build fifo_test",
                "Build sort_test"
            ],
            "command": "echo",
            "args": [
                "Build",
                "Done!"
            ],
            "options": {
                "cwd": "${workspaceFolder}/Examples"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Build all binary"
        },
        {
            "type": "shell",
            "label": "Clean all",
            "command": "git clean -dXf",
            "options": {
                "cwd": "${workspaceFolder}/Examples"
            },
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "detail": "Clean all binary"
        }
    ],
    "version": "2.0.0"
}